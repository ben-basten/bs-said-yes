    "typecheck": "nuxt typecheck",
    "prepare": "husky"
  },
  "dependencies": {
    "@nuxt/eslint": "^1.9.0",
    "@nuxt/fonts": "^0.11.4",
    "@tailwindcss/vite": "^4.1.18",
    "contentful": "^11.10.2",
    "contentful-rich-text-vue-renderer": "^3.2.1",
    "eslint": "^9.39.2",
    "nuxt": "^4.2.2",
    "tailwindcss": "^4.1.18",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@contentful/rich-text-types": "^17.2.5",
    "@types/node": "^24.10.9",
    "husky": "^9.1.7",
    "nano-staged": "^0.9.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.2.0"
  },
  "nano-staged": {
    "*.{js,mjs,jsx,ts,tsx}": [
